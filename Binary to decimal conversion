// Binary to decimal conversion 
#include<iostream>
using namespace std;
int main(){

    int n;
    cout<< "Enter Binary number, you want ot convert into Decimal :";
    cin>>n;

    int ans=0;
    int power =1;

    while(n>0){
        int lastDigit= n % 10;

        ans = ans + (lastDigit*power);
        power= power*2;
        n = n / 10;
        


    } cout <<ans <<endl;



    return 0;
}
