class Solution {
public:
    int xorOperation(int n, int start) {
        std::vector<int>nums;
        for(int i=0; i<n; i++){
            nums.push_back(start + 2*i);

        }

        int xors=0;
        for(int i=0; i<nums.size();i++){
            xors = xors^nums[i];


        }
         return xors;
    }
};
